<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AudioProgressBar">

        <attr name="primary_color" format="reference|color" /><!--无信号颜色-->
        <attr name="progress_color" format="reference|color" /><!--有信号颜色-->
        <attr name="spacing" format="dimension" /><!--间隙-->
        <attr name="unit_width" format="dimension" /><!--柱宽度-->
        <attr name="barStyle" format="integer">
            <flag name="normal" value="0" />
            <flag name="dynamic" value="1" />
        </attr>

    </declare-styleable>
</resources>